<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  name="Suite">

    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

   
       <test thread-count="5" name="Run Regression Group">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>



  
         <classes>
            <class name="tests.Testcase1_RegisterTest"/>
            <class name="tests.Testcase2_LoginTest"/>
            <class name="tests.Testcase3_DashboardTest"/>
            <class name="tests.Testcase4_SendEmailTest"/>
            <class name="tests.Testcase5_ContactUsTest"/>
            <class name="tests.Testcase6_WishlistTest"/>
        </classes>
    </test>
  

  
</suite> <!-- Suite -->


